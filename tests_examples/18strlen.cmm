fun strlen (string str) : int
{
    int i = 0;
    while (str[i] != '\0') {
        i++;
    }
    return i;
}

fun reverse (string str) : string
{
    int len = strlen(str);
    string rev = "";
    for (int i = 0; i < len; i++) {
        rev += str[len - i - 1];
    }
    return rev;
}

fun str_first_n_char (string src, int n) : string
{
    string dest = "";
    for (int i = 0; i < n; i++) {
        dest += src[i];
    }
    return dest;
}

fun str_last_n_char (string src, int n) : string
{
    int len = strlen(src);
    string dest = "";
    for (int i = 0; i < n; i++) {
        dest += src[len - n + i];
    }
    return dest;
}

