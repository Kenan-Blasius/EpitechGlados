// 2

fun add (int a) : int
{
    return a + 1;
}

fun sub(int a, int b) : int
{
    return a - b;
}

fun main () : int
{
    int a = 0;
    int b = 4;
    if (
        add(1) == sub(a, b) // if 2 == -4
    ) {
        a = 42;
    } else if (
        add(a) == add(0) // if 1 == 1
    ) {
        a = sub(a, b); // a = -4
        while (b < 10) {
            a = add(a); // a = -3, -2, -1, 0, 1, 2
            b++;
        }
    } else {
        a = 1;
    }


    return a;
}
